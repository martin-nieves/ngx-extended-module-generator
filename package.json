{
  "name": "ngx-extended-module-generator",
  "version": "1.0.1",
  "description": "Generate an angular module with an extended base structure by executing multiple Angular CLI scaffolding commands at once based on module name",
  "main": "./src/bin/index.ts",
  "scripts": {
    "start": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/bin/index.js"
  },
  "bin": {
    "ngex": "./dist/bin/index.js"
  },
  "dependencies": {
    "@types/node": "^12.0.10",
    "@types/pluralize": "0.0.29",
    "colors": "^1.3.3",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martin-nieves/ng-extended-module-generator.git"
  },
  "author": "martin-nieves",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/martin-nieves/ng-extended-module-generator/issues"
  },
  "homepage": "https://github.com/martin-nieves/ng-extended-module-generator#readme"
}
